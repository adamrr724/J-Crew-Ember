<div class="subNav">
  <h3>Shopping Cart</h3>
</div>
<ul>
  {{#each shoppingCart.items as |item|}}
    <li>{{item.name}} -- {{item.price}}</li>
    <button {{action 'remove' item}}>Remove from Cart</button>
  {{/each}}
  <p><strong>Total Price:</strong> ${{total-price shoppingCart.items}}.00</p>
</ul>
